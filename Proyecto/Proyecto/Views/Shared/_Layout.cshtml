<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- Header -->
    <header id="header" class="alt">
        <div class="logo"><a href="http://localhost:49999/home/index">El Progreso</a></div>
        <a href="#menu">Menu</a>
    </header>

    <!-- Nav -->
    <nav id="menu">
        <ul class="links">
            <li><a href="http://localhost:49999/home/index">Inicio</a></li>
            <li><a href="http://localhost:49999/Indices/tasaPolíticaMonetaria" class="Especifico">Tasa de política monetaria</a></li>
            <li><a href="http://localhost:49999/Indices/tasaBasicaPasiva" class="Especifico">Tasa básica pasiva</a></li>
            <li><a href="http://localhost:49999/Indices/ventaDolar" class="Especifico">Venta dolar</a></li>
            <li><a href="http://localhost:49999/Indices/compraDolar" class="Especifico">Compra dolar</a></li>
            <li><a href="http://localhost:49999/detalleIndicador/detalleIndicador" class="Especifico">Detalle indicador</a></li>
            <li><a href="http://localhost:49999/consulta/consulta" class="Especifico">Foro de consultas</a></li>
        </ul>
    </nav>
</head>
<body>
    @RenderBody()
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    

</body>
<!-- Footer -->
<footer id="footer">
    <div class="container">
        <ul class="icons">
            <li><a href="#" class="icon fa-twitter"><span class="label">Twitter</span></a></li>
            <li><a href="#" class="icon fa-facebook"><span class="label">Facebook</span></a></li>
            <li><a href="#" class="icon fa-instagram"><span class="label">Instagram</span></a></li>
            <li><a href="#" class="icon fa-envelope-o"><span class="label">Email</span></a></li>
        </ul>
    </div>
    <div class="copyright">
        &copy; Todos los derechos reservados 2019.
    </div>
</footer>
</html>
